{
  "name": "@nxtedition/rocksdb",
  "version": "12.0.6",
  "description": "A low-level Node.js RocksDB binding",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "install": "node-gyp-build",
    "test": "standard && (nyc -s tape test/*-test.js | faucet) && nyc report",
    "test-prebuild": "cross-env PREBUILDS_ONLY=1 npm t",
    "rebuild": "JOBS=8 npm run install --build-from-source"
  },
  "dependencies": {
    "abstract-level": "^1.0.2",
    "catering": "^2.1.1",
    "module-error": "^1.0.2",
    "napi-macros": "~2.2.2",
    "node-gyp-build": "^4.8.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.3",
    "@voxpelli/tsconfig": "^4.0.0",
    "async-each": "^1.0.3",
    "cross-env": "^7.0.3",
    "delayed": "^2.0.0",
    "dependency-check": "^4.1.0",
    "du": "^1.0.0",
    "faucet": "^0.0.3",
    "glob": "^8.0.3",
    "lru-cache": "^11.0.0",
    "mitata": "^0.1.11",
    "mkfiletree": "^2.0.0",
    "node-gyp": "^9.3.0",
    "nyc": "^15.0.0",
    "readfiletree": "^1.0.0",
    "rimraf": "^3.0.0",
    "standard": "^17.0.0",
    "tape": "^5.7.5",
    "tempy": "^1.0.1"
  },
  "standard": {
    "ignore": [
      "deps/*"
    ]
  },
  "gypfile": true,
  "engines": {
    "node": ">=17.0.0"
  }
}
